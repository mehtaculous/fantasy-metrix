<div class="row">
  <div class="col-md-12">
    <h3>2015 Tight End Leaders</h3>
    <table id="header" class="table table-bordered grey text-center">
      <tbody>
        <tr>
          <th colspan="2">Player</th>
          <th colspan="7">Receiving</th>
          <th colspan="3">Rushing</th>
          <th colspan="2">Fantasy Points</th>
        </tr>
        <tr>
          <th title="Name">NAME</th>
          <th title="Team">TEAM</th>
          <th title="Targets">TGTS</th>
          <th title="Receptions">REC</th>
          <th title="Receiving Yards">YARDS</th>
          <th title="Yards Per Game">YPG</th>
          <th title="Yards Per Reception">YPR</th>
          <th title="100+ Receiving Yard Games">100+</th>
          <th title="Receiving Touchdowns">TD</th>
          <th title="Rushing Attempts">RUSH</th>
          <th title="Rushing Yards">YARDS</th>
          <th title="Rushing Touchdowns">TD</th>
          <th title="Standard Scoring">STANDARD</th>
          <th title="Points Per Reception Scoring">PPR</th>
        </tr>
        <% @players.each do |player| %>
          <tr>
            <% if player.tight_end? %>
              <td align="left"><%= link_to player.name, player_path(player), class: 'player-stats' %></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>