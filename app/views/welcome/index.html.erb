<div class="home"><br><br><br><br>
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
      <div class="jumbotron text-center">
        <h1 class="title">FantasyMetrix</h1><br>
        <form class="navbar-form" role="search">
        <div class="input-group">
          <input type="text" class="form-control" placeholder="Search NFL Player" name="srch-term" id="srch-term">
          <div class="input-group-btn">
            <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
          </div>
        </div>
        </form><br>
        <% if !current_user %>
          <p><%= link_to "Sign Up", new_user_registration_path, class: 'index btn btn-success' %></p>
        <% end %><br>
        <%= link_to image_tag("FanDuel.jpg", :width => 150, :height => 150, :alt => "FanDuel"), "https://www.fanduel.com/q1", :target => "_blank", class: "fanduel" %>
        <%= link_to image_tag("DraftKings.jpg", :width => 150, :height => 150, :alt => "DraftKings"), "https://www.draftkings.com/", :target => "_blank", class: "draftkings" %>
      </div><br><br><br>
    </div>
  </div>
</div>