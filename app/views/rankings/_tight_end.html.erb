<div class="row">
  <div class="col-md-12">
    <h3>Tight End - Draft Rankings</h3>
    <table id="header" class="table table-bordered grey text-center">
      <tbody>
        <tr>
          <th title="Player Ranking">Rank</th>
          <th title="Player Name">Player</th>
          <th title="Team Name">Team</th>
          <th>Sahil Mehta</th>
          <th>Evan Silva</th>
          <th>Matt Harmon</th>
          <th>Arun Alagappan</th>
        </tr>
        <% @nfl_players.each do |nfl_player| %>
          <tr>
            <% if nfl_player.tight_end? %>
              <td style="width: 100px"><%= nfl_player.rank %></td>
              <td style="text-align:left"><%= link_to nfl_player.name, nfl_player_path(nfl_player), class: 'player-stats' %></td>
              <td style="width: 100px" class="green"><%= link_to nfl_player.nfl_team.abbreviation, nfl_team_path(nfl_player.nfl_team.id), class: 'player-stats' %></td>
              <td style="width: 140px"></td>
              <td style="width: 140px"></td>
              <td style="width: 140px"></td>
              <td style="width: 140px"></td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>