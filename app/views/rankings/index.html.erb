      <button class="devise btn btn-success dropdown-toggle" type="button" data-toggle="dropdown">Rankings <span class="caret"></span></button>
      <ul class="dropdown-menu">
        <li><%= link_to "Quarterbacks", rankings_path(position: 'quarterbacks') %></li>
        <li><%= link_to "Running Backs", rankings_path(position: 'running-backs') %></li>
        <li><%= link_to "Wide Receivers", rankings_path(position: 'wide-receivers') %></li>
        <li><%= link_to "Tight Ends", rankings_path(position: 'tight-ends') %></li>
      </ul>
    </div>
  </div>
</div><br>
<div>
  <%= render partial: @position.gsub(' ', '_').downcase, locals: { nfl_players: @nfl_players } %>
</div>