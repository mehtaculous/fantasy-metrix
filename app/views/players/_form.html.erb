<%= form_for [team, player] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', placeholder: "Enter player name" %>
  </div>
  <div class="form-group">
    <%= f.label :age %>
    <%= f.text_field :age, class: 'form-control', placeholder: "Enter age" %>
  </div>
  <div class="form-group">
    <%= f.label :college %>
    <%= f.text_field :college, class: 'form-control', placeholder: "Enter college" %>
  </div>
  <div class="form-group">
    <%= f.label :draft %>
    <%= f.text_field :draft, class: 'form-control', placeholder: "Enter draft pick" %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: 'devise btn btn-success' %>
  </div>
<% end %>